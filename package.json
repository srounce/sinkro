{
  "name": "sinkro",
  "version": "1.2.1",
  "description": "A small helper for plugging ES Observables into React components",
  "main": "lib/index.js",
  "repository": "git@gitlab.com:srounce/sinkro.git",
  "author": "Samuel Rounce <me@samuelrounce.co.uk>",
  "keywords": [
    "Observable",
    "React",
    "RxJS",
    "Functional",
    "Reactive",
    "Programming",
    "state",
    "redux",
    "stream",
    "declarative"
  ],
  "scripts": {
    "build": "yarn babel -d lib/ -D --source-maps true --ignore '**.spec' src",
